  FBLD B 1 A_SpawnItemEx("NewFlyingBloodTrail",0,0,0,0,0,0,0,129)
  FBLD B 1
  FBLD B 1 A_SpawnItemEx("NewFlyingBloodTrail",0,0,0,0,0,0,0,129)
  FBLD B 1
  FBLD C 1 A_SpawnItemEx("NewFlyingBloodTrail",0,0,0,0,0,0,0,129)
  FBLD C 1
  FBLD C 1 A_SpawnItemEx("NewFlyingBloodTrail",0,0,0,0,0,0,0,129)
  FBLD C 1
  FBLD D 1 A_SpawnItemEx("NewFlyingBloodTrail",0,0,0,0,0,0,0,129)
  FBLD D 1
