}

ACTOR NewBloodSpot
{
Game Doom
Radius 12
Height 2
Mass 1
+NOTELEPORT
-NOBLOCKMAP
decal BloodSplat
States
{
Spawn:
  TNT1 A 0
  TNT1 A 0
  TNT1 A 0 A_Jump(64,4)
  TNT1 A 0 A_Jump(128,5)
  TNT1 A 0 A_Jump(192,6)
  TNT1 A 0 A_Jump(255,7)
  BSPT A 250
  BSPT A 1 A_FadeOut (0.02)
  goto Spawn+7
  BSPT B 250
  BSPT B 1 A_FadeOut (0.02)
  goto Spawn+9
  BSPT C 250
  BSPT C 1 A_FadeOut (0.02)
  goto Spawn+11
  BSPT D 250
  BSPT D 1 A_FadeOut (0.02)
  goto Spawn+13
  }
}

